{
  "name": "nextjs-typescript-graphql-starter",
  "version": "1.0.0",
  "description": "Starter nextjs project configured for typescript, apollo graphql, styled-system and storybook.",
  "author": "Thomas Gorny",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check-types": "tsc --noEmit",
    "graphql:schema": "apollo service:download --endpoint=https://api.graph.cool/simple/v1/cixmkt2ul01q00122mksg82pn",
    "graphql:types": "apollo client:codegen --includes=**/*.tsx --excludes=node_modules --target=typescript generated",
    "graphql:types:watch": "yarn graphql:types --watch"
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.12",
    "graphql": "^14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "polished": "^2.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/graphql": "^14.0.5",
    "@types/jest": "^23.3.13",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.19",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.11",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.6",
    "@types/styled-system": "^3.1.3",
    "apollo": "^2.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "jest": "^24.0.0",
    "prettier": "^1.16.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "3.2.4"
  }
}
