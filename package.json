{
	"name": "nextjs-typescript-graphql-starter",
	"version": "1.0.0",
	"description": "Starter nextjs project configured for typescript, apollo graphql, jss and storybook.",
	"author": "Thomas Gorny",
	"license": "MIT",
	"scripts": {
		"dev": "concurrently \"yarn graphql:types:watch\" \"next\"",
		"build": "NODE_ENV=production next build",
		"start": "NODE_ENV=production next start",
		"test": "jest",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"check-types": "tsc --noEmit",
		"graphql:schema": "apollo client:download-schema --endpoint=https://api.graph.cool/simple/v1/cixmkt2ul01q00122mksg82pn --config=./apollo.config.dummy.js",
		"graphql:types": "apollo client:codegen --includes=\"src/**/*.{ts,tsx}\" --excludes=node_modules --target=typescript generated",
		"graphql:types:watch": "yarn graphql:types --watch"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.0",
		"@apollo/react-ssr": "^3.1.0",
		"apollo-cache-inmemory": "^1.6.3",
		"apollo-client": "^2.6.4",
		"apollo-link-http": "^1.5.16",
		"clsx": "^1.0.4",
		"graphql": "^14.5.4",
		"graphql-tag": "^2.10.1",
		"isomorphic-unfetch": "^3.0.0",
		"next": "^9.0.5",
		"polished": "^3.4.1",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-jss": "^10.0.0-alpha.24",
		"ts-node": "^8.3.0",
		"typescript": "3.6.3"
	},
	"devDependencies": {
		"@babel/core": "^7.6.0",
		"@storybook/addon-actions": "^5.1.11",
		"@storybook/addon-links": "^5.1.11",
		"@storybook/addons": "^5.1.11",
		"@storybook/react": "^5.1.11",
		"@material-ui/styles": "^4.4.1",
		"@types/jest": "^24.0.18",
		"@types/node": "^12.7.5",
		"@types/react": "^16.9.2",
		"@types/react-dom": "^16.9.0",
		"@types/storybook__react": "^4.0.2",
		"apollo": "^2.18.1",
		"babel-loader": "^8.0.6",
		"concurrently": "^4.1.2",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"ts-jest": "^24.0.2",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^8.0.1"
	}
}
