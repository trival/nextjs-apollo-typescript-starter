{
  "name": "nextjs-typescript-graphql-starter",
  "version": "1.0.0",
  "description": "Starter nextjs project configured for typescript, apollo graphql, jss and storybook.",
  "author": "Thomas Gorny",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn graphql:types:watch\" \"next\"",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "graphql:schema": "apollo client:download-schema --endpoint=https://api.graph.cool/simple/v1/cixmkt2ul01q00122mksg82pn --config=./apollo.config.dummy.js",
    "graphql:types": "apollo client:codegen --includes=\"src/**/*.{ts,tsx}\" --excludes=node_modules --target=typescript generated",
    "graphql:types:watch": "yarn graphql:types --watch",
    "graphql:update": "yarn graphql:schema && yarn graphql:types"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "clsx": "^1.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "jss-style-helpers": "^0.1.0",
    "next": "^9.3.3",
    "polished": "^3.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "ts-node": "^8.8.1",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "apollo": "^2.25.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0"
  }
}
